{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".expo/**"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "type-check": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "ios": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "android": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "web": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "db:push": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "db:migrate": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "db:studio": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test:e2e": {
      "dependsOn": ["^build"],
      "outputs": []
    }
  }
}
